* {
    red:                         rgba ( 240, 113, 120, 100 % );
    blue:                        rgba ( 130, 170, 255, 100 % );
    lightfg:                     rgba ( 149, 157, 203, 100 % );
    lightbg:                     rgba ( 68, 66, 103, 100 % );
    foreground:                  rgba ( 149, 157, 203, 100 % );
    background:                  rgba ( 41, 45, 62, 100 % );
    background-color:            rgba ( 41, 45, 62, 0 % );
    separatorcolor:              @foreground;
    border-color:                @foreground;
    selected:                    @lightfg;
    urgent:                      @background;
}
